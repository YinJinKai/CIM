<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.leaveWords.model">

	<class name="com.leaveWords.model.LeaveWordsModel" table="leaveWords">
	    <id name="id" column="id" type="int">
              <!-- 指定对象标识符生成器：class属性指定生成器的类别名 -->
            <generator class="native"/>
        </id>
		     <!-- 逻辑主键学院编号 -->
           <property
            name="email"
            column="email"
            type="string"
            length="20"
          />
           <property
            name="sumbitDate"
            column="sumbitDate"
            type="string"
            length="200"
          />
           <property
            name="leaveContent"
            column="leaveContent"
            type="string"
            length="300"
          />
          </class>
          </hibernate-mapping>