<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.student.model">
	<class name="com.student.model.StudentModel" table="student">
		<id name="id" column="id" type="int">
			<!-- 指定对象标识符生成器：class属性指定生成器的类别名 -->
			<generator class="native" />
		</id>
		 <property
            name="studentNumber"
            column="studentNumber"
            type="string"
            length="20"
          />
           <property
            name="studentName"
            column="studentName"
            type="string"
            length="20"
          />
           <property
            name="studentPassword"
            column="studentPassword"
            type="string"
            length="20"
          />
           <property
            name="studentSex"
            column="studentSex"
            type="string"
            length="20"
          />
           <property
            name="studentClassNumber"
            column="studentClassNumber"
            type="string"
            length="20"
          />
           <property
            name="studentBirthday"
            column="studentBirthday"
            type="string"
            length="20"
          />
           <property
            name="studentState"
            column="studentState"
            type="string"
            length="20"
          />
           <property
            name="studentPhoto"
            column="studentPhoto"
            type="string"
            length="200"
          />
           <property
            name="studentTelephone"
            column="studentTelephone"
            type="string"
            length="20"
          />
           <property
            name="studentEmail"
            column="studentEmail"
            type="string"
            length="200"
          />
           <property
            name="studentAddress"
            column="studentAddress"
            type="string"
            length="200"
          />
           <property
            name="studentMemo"
            column="studentMemo"
            type="string"
            length="200"
          />
	</class>
</hibernate-mapping>